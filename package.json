{
  "name": "imotaraapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "IMOTARA_PRUNE_DEV_ROUTES=1 node scripts/devRoutes.mjs prune",
    "build": "next build",
    "postbuild": "IMOTARA_PRUNE_DEV_ROUTES=1 node scripts/devRoutes.mjs restore",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check": "npm run typecheck && npm run build",
    "donation:check": "node -e \"fetch('http://localhost:3000/api/payments/donation-intent',{method:'POST',headers:{'content-type':'application/json'},body:JSON.stringify({presetId:'inr_99',purpose:'imotara_donation',platform:'web'})}).then(r=>r.json()).then(j=>{console.log(j); if(!j.ok) process.exit(1);}).catch(e=>{console.error(e); process.exit(1);});\""
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.11.1",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.78.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.552.0",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.3.0",
    "stripe": "^20.0.0",
    "tailwind-variants": "^3.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  }
}